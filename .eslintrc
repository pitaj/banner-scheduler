{
  "globals": {
    "require": true,
    "$": true
  },
  "env": {
    "node": true
  },
  "rules": {
    "quotes": 0,
    "no-mixed-requires": 0,
    "no-comma-dangle": 0,
    "space-infix-ops": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-trailing-spaces": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "no-extra-strict": 0
  }
}
